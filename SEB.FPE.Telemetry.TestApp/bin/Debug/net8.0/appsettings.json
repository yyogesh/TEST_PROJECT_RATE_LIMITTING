{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "SEB.FPE.Telemetry": "Information"
    }
  },
  "AllowedHosts": "*",
  "Telemetry": {
    "IsEnabled": true,
    "LogRequests": true,
    "LogResponses": true,
    "LogExceptions": true,
    "MaxRequestBodySize": 10240,
    "MaxResponseBodySize": 10240,
    "ExcludedPaths": "/swagger",
    "ExcludedHttpMethods": "",
    "IncludeRequestHeaders": true,
    "IncludeResponseHeaders": false,
    "SensitiveHeaders": "Authorization,Api-Key,X-API-Key,Password",
    "SensitiveQueryParameters": "password,token,apikey,secret",
    "LogBody": true,
    "ExcludedContentTypes": "application/octet-stream,image/,video/,audio/",
    "ApplicationInsightsConnectionString": "",
    "LogDestination": "Local",
    "WriteToLocalLogs": true,
    "WriteToApplicationInsights": false,
    "FileLogging": {
      "IsEnabled": true,
      "LogDirectory": "Logs",
      "LogFileName": "telemetry",
      "LogFileExtension": ".log",
      "RotateByDay": true,
      "MaxFileSizeBytes": 10485760,
      "MaxFilesToKeep": 30
    }
  }
}
